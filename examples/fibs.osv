%include "sys_libs.osv"

_start: 
    mov r0, #17
    
    push #0
    call sprintn
    
    push #1
    call sprintn
loop:
    dupl 1
    dupl 1
    adds
    
    ; Print fibbonacci number using print_num system function
    call sprintn
    
    ; Decrement the counter
    dec r0
    jnz loop, r0
    hlt