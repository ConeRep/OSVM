%include "sys_libs.osv"

should_never_run:
    push #69
    push #69
    adds

_start:
    ; counter
    mov r15, #17
    
    mov r0, #0
    mov r16, r0
    call rprintn
    
    mov r1, #1
    mov r16, r1
    call rprintn
loop:
    add r0, r0, r1
    mov r16, r0
    srg r0, r1
    
    ; Print fibbonacci number using rprintn system function
    call rprintn
    
    ; Decrement counter
    dec r15
    jnz loop, r15
    hlt